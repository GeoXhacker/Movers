    "react-datetime-picker": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-id-swiper": "^4.0.0",
    "react-mapbox-gl": "^5.1.1",
    "react-mapbox-gl-geocoder": "^1.1.0",
    "react-redux": "^7.2.4",
